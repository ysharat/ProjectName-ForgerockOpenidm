{
    "enabled" : true,
    "path" : "config/*",
    "methods" : [
        "update",
        "patch"
    ],
    "condition" : "resourcePath eq 'selfservice.terms'",
    "target" : {
        "resource" : "managed/user",
        "_queryFilter" : "true"
    },
    "notification" : {
        "notificationType" : "info",
        "message" : "We have updated our Terms and Conditions."
    }
}